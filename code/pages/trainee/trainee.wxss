/* trainee.wxss */
.trainee__avatar {
  width:92rpx;
  height:92rpx;
  border-radius:50%;
  margin-top:6rpx;
  margin-bottom:-6rpx;
}

.weui-cell__ft{
  color: #999;
  font-size: 26rpx;
}

.trainee{
  display: inline-block;
  height: 85rpx;
  margin: 0 48rpx;
  float: left;
}

.trainee__name{
  display: block;
  width: 200rpx;
  height: 48rpx;
  line-height: 48rpx;
  color: #222;
  font-size: 34rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.trainee__phone{
  display: block;
  width: 200rpx;
  height: 48rpx;
  line-height: 48rpx;
  color: #c7c7cc;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.weui-cell__bd {
  width: 20%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
